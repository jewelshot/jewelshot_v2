{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@jewelshot_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@jewelshot_supabase_anon_key",
    "SUPABASE_SERVICE_ROLE_KEY": "@jewelshot_supabase_service_role_key",
    "FAL_KEY": "@jewelshot_fal_key",
    "NEXT_PUBLIC_APP_URL": "@jewelshot_app_url"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
